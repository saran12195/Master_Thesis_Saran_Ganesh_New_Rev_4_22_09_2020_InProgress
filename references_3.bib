@inproceedings{erlich_control_2017,
	address = {Chicago, IL},
	title = {Control challenges in power systems dominated by converter interfaced generation and transmission technologies},
	isbn = {978-1-5386-2212-4},
	url = {http://ieeexplore.ieee.org/document/8273862/},
	doi = {10.1109/PESGM.2017.8273862},
	abstract = {This paper surveys the control challenges arising from the increasing level of power injected into the network through converter interfaces. As is well-known, converters link renewable based power generation plants as well as voltage source converter based high voltage DC transmission lines (VSCHVDC) to the main grid. Using conceptual analysis supported by simulation results on a representative test network, it is shown that current injection control, as it is implemented in almost all wind turbines (WT) controls at the moment, is not suitable in grids dominated by converter in-feeds such as offshore wind farms. A new WT as well as VSC – HVDC control concept, which determines the converter reference voltage directly without the need for an underlying current controller, has been proposed. Additionally, alternative options for frequency support by the HVDC terminals that can be incorporated into the active power control channel have been presented and discussed. Simulation results show that the control approach fulfils all the operational control functions in steady state and in contingency situations supporting fault ride-through and emergency frequency support, without encountering the problems arising from current injection control.},
	urldate = {2020-06-13},
	booktitle = {2017 {IEEE} {Power} \& {Energy} {Society} {General} {Meeting}},
	publisher = {IEEE},
	author = {Erlich, I. and Korai, A. and Shewarega, F.},
	month = jul,
	year = {2017},
	pages = {1--5}
}

